<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper
                  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
             <mapper namespace="com.wangmiao.demo.dao.UserMapper">
            <select id="selectByUserId" resultType="com.wangmiao.demo.pagemodel.req.UserRes">
               SELECT
                 id AS Id,
                 name,
                 age,
                  sex
                FROM sybnew.t_user
                WHERE id = #{userId}
            </select>
    </mapper>